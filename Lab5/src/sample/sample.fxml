<GridPane alignment="center" hgap="10" prefHeight="670.0" prefWidth="959.0" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="500.0" prefWidth="500.0">
         <children>
            <ImageView fitHeight="727.0" fitWidth="1045.0" layoutX="-19.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@space-03.jpg" />
               </image>
            </ImageView>
            <ChoiceBox id="Choice" fx:id="choices" layoutX="364.0" layoutY="81.0" onAction="#folderByCategory" prefHeight="37.0" prefWidth="162.0" />
            <TextArea fx:id="inputText" layoutX="367.0" layoutY="149.0" prefHeight="107.0" prefWidth="273.0" promptText="Входные данные">
               <font>
                  <Font size="14.0" />
               </font></TextArea>
            <TextArea fx:id="outputText" layoutX="371.0" layoutY="370.0" prefHeight="187.0" prefWidth="291.0" promptText="Результат">
               <font>
                  <Font size="14.0" />
               </font></TextArea>
            <Button fx:id="taskButton" layoutX="367.0" layoutY="301.0" mnemonicParsing="false" onAction="#doTask" prefHeight="27.0" prefWidth="215.0" text="Выполнить задание" textOverrun="CENTER_WORD_ELLIPSIS">
               <font>
                  <Font name="Apple Color Emoji" size="14.0" />
               </font>
            </Button>
            <Button fx:id="clearBut" layoutX="373.0" layoutY="598.0" mnemonicParsing="false" onAction="#clearField" prefHeight="35.0" prefWidth="144.0" text="Очистить" />
            <Button fx:id="exitBut" layoutX="524.0" layoutY="598.0" mnemonicParsing="false" onAction="#exit" prefHeight="35.0" prefWidth="144.0" text="Выход" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
